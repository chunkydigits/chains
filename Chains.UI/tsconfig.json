{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "moduleResolution": "node",
        "sourceMap": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "lib": [
            "es2016",
            "dom", 
            "es5"
        ],
        "removeComments": false,
        "noImplicitAny": false,
        "outDir": "dist",
        "allowSyntheticDefaultImports": true
    }
}